.photosGroupView {
  width: 380px
  line-height: 0

  > .image {
    display: inline-block
    background-repeat: no-repeat
    background-size: cover
    background-position: center center

    &.full {
      width: 380px
      height: 253px
      margin-bottom: 7px
    }

    &.row {
      margin-bottom: 7px
      &:not(:last-child) {
        margin-right: 7px
      }
    }
  }
}
